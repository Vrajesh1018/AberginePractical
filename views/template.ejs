<!DOCTYPE html>
<html>

<head>
  <title>Profile Card</title>
  <style>
    /* Basic CSS for the profile card */
    .profile-card {
      width: 300px;
      padding: 20px;
      border: 1px solid #ccc;
      border-radius: 5px;
      box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
    }

    .profile-photo {
      width: 150px;
      height: 150px;
      border-radius: 50%;
      overflow: hidden;
      margin: 0 auto 20px;
    }

    .profile-photo img {
      width: 100%;
      height: 100%;
      object-fit: cover;
    }

    .profile-name {
      font-size: 24px;
      font-weight: bold;
      text-align: center;
      margin-bottom: 10px;
    }

    .profile-email {
      font-size: 16px;
      text-align: center;
    }
  </style>
</head>

<body>

  <% JSONdata.forEach((ele)=>{ %>
    <div class="profile-card">
      <div class="profile-photo">
        <img src="<%= ele.avatar %>" alt="Profile Photo">
      </div>
      <div class="profile-name"><%= ele.first_name %>  <%= ele.last_name %></div>
      <div class="profile-email"><%= ele.email %></div>
    </div>

    <% }) %>
</body>

</html>